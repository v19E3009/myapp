<% if session[:user_id] %>
    <%= link_to 'サインアウト', post_path, class 'btn btn-sm btn-primary' %>
    <%= link_to 'サインアップ', post_path, class 'btn btn-sm btn-secondary' %>
<% else %>
    <% if User.all.any? %>
        <%= link_to 'サインイン', post_path, class 'btn btn-sm btn-primary' %>
    <% else %>
        <%= link_to 'サインアップ', post_path, class 'btn btn-sm btn-secondary' %>
    <% end %>
<% end %>